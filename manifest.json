{
    "manifest_version": 3,
    "name": "RSS Notifier",
    "version": "0.1",
    "description": "Get notifications for new RSS feed posts.",
    "permissions": [
      "notifications",
      "storage",
      "alarms"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "options_page": "options.html"
  }  